# Example env vars (do not commit real secrets)
# Copy to .env and fill in values as needed

# BrAPI token (set only in Render or local .env)
BRAPI_TOKEN=

# Cache controls (optional)
CACHE_TTL_MS=300000
MAX_CACHE_ENTRIES=500

# Health/diagnostics
ENABLE_CACHE_STATS=false

# CORS controls
ENABLE_CORS_RESTRICT=false
ALLOWED_ORIGINS=

# Rate limiting controls
ENABLE_RATE_LIMIT=true
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=60
